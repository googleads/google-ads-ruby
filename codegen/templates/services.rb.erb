module Google
  module Ads
    module GoogleAds
      module Factories
        module Services
          <% services.each do |service, path| %>
          def self.<%= service.name.split("::").last.underscore.gsub("_service_client", "") %>
            require "<%= path.split("google-ads-ruby/lib/").last %>"
            <%= service.name %>.new
          end
          <% end %>
        end
      end
    end
  end
end
